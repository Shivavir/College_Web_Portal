<html>
<head>
    
    <title>Change Password</title>
     <%@ include file="common/links.jsp" %>
     <style>
       .form-control
       {
         margin: 30px 0px;
       }
       
    
.star-rating {
   margin: 25px 0 0px;
  font-size: 0;
  white-space: nowrap;
  display: inline-block;
  width: 175px;
  height: 35px;
  overflow: hidden;
  position: relative;
  background: url('data:image/svg+xml;base64,');
  background-size: contain;
}
.star-rating i {
  opacity: 0;
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  width: 20%;
  z-index: 1;
  background: url('data:image/svg+xml;base64,');
  background-size: contain;
}
.star-rating input {
  -moz-appearance: none;
  -webkit-appearance: none;
  opacity: 0;
  display: inline-block;
  width: 20%;
  height: 100%;
  margin: 0;
  padding: 0;
  z-index: 2;
  position: relative;
}
.star-rating input:hover + i,
.star-rating input:checked + i {
  opacity: 1;
}
.star-rating i ~ i {
  width: 40%;
}
.star-rating i ~ i ~ i {
  width: 60%;
}
.star-rating i ~ i ~ i ~ i {
  width: 80%;
}
.star-rating i ~ i ~ i ~ i ~ i {
  width: 100%;
}	 </style>
</head>
<body>
    
    <div class="container-fluid">
       <%@ include file="common/header.jsp" %>
     
        <!-- Feedback Block -->
        
        <div class="row">
          <div class="col-sm-4"></div>
          <div class="col-sm-4" style="margin: 30px 0px;" >
          <h1 class="text-center text-primary">Feedback</h1>
          <h4 class="text-center text-success">Please give Your Feedback</h4>
             <div class="row" style="margin: 35px 0px;" >
               <div class="col-sm-12 bg-info" id="feedbox" style="border-radius: 15px;">
                 <form method="get" action="savefeedback.jsp">
<center><span  class="star-rating">
  <input type="radio" name="rating" value="1"><i></i>
  <input type="radio" name="rating" value="2"><i></i>
  <input type="radio" name="rating" value="3"><i></i>
  <input type="radio" name="rating" value="4"><i></i>
  <input type="radio" name="rating" value="5"><i></i>
</span>
</center>
                 <textarea class="form-control" id="feedmsg" name="feedmsg" 
                 placeholder="Type Your Feedback Here ..." rows="7" style="resize:none;"></textarea>
                 <button type="submit" class="btn btn-success btn-block">Save Feedback</button>
                 </form>
               </div>
             </div>
          </div>
        </div>
                <%@ include file="common/footer.jsp" %>

</body>
</html>